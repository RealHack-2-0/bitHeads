@model ASPNET_Core_2_1.Models.QuestionResponse
@{
    ViewData["Title"] = "QuestionList";
}



<html>
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

</head>
<body>
    <ul>
        @foreach (var data in Model.Data)
        {

        <div class="jumbotron">
            <h1><a asp-action="QuestionDetail" asp-controller="QnA" asp-route-id=@data.QuestionId>@data.Title</a></h1>
            
            <p class="lead">@data.Content</p>
            <a class="btn btn-lg btn-primary" role="button">@data.UpvoteCount</a>
            <a class="btn btn-lg btn-danger" role="button">@data.DownvoteCount</a>
        </div>
        }
    </ul>
</body>
</html>